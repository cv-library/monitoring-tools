# hourly
./script.sh; curl -X POST http://localhost:5001/v1/post_metric \
-H "Content-Type: application/json" \
-d "{\"app_name\": \"cron_test_hourly_14\", \"metric_name\": \"cron_execution\", \"value\": $? , \"schedule\": \"25******\"}"



> ./script_failed.sh; curl -X POST http://localhost:5001/v1/post_metric \
-H "Content-Type: application/json" \
-d "{\"app_name\": \"cron_test_hourly_14\", \"metric_name\": \"cron_execution\", \"value\": $? , \"schedule\": \"25******\"}"


